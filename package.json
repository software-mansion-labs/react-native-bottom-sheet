{
  "name": "@swmansion/react-native-bottom-sheet",
  "version": "0.4.0",
  "description": "Provides bottom-sheet components for React Native.",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace @swmansion/react-native-bottom-sheet-example",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "android",
    "ios"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion-labs/react-native-bottom-sheet.git"
  },
  "author": "Software Mansion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/software-mansion-labs/react-native-bottom-sheet/issues"
  },
  "homepage": "https://github.com/software-mansion-labs/react-native-bottom-sheet",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.1",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@types/react": "19.2.14",
    "del-cli": "6.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "prettier": "2.8.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-builder-bob": "0.40.18",
    "react-native-gesture-handler": "2.28.0",
    "react-native-reanimated": "4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-worklets": "0.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.76.0",
    "react-native-gesture-handler": ">=2.14.0",
    "react-native-reanimated": ">=3.16.0",
    "react-native-safe-area-context": ">=4.0.0",
    "react-native-worklets": ">=0.4.0"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
